@model TopicsViewModel
<h3 class="mt-3 mb-2">Please feel free to add any suggestions or addtional feedback here!</h3>
<partial name="_Notification" />

<form method="post">
    <div class="pt-2">
        
        <textarea id="Feedback" name="Feedback" rows="6" cols="126" placeholder=" additional feedback here..." class="mt-2 mb-2 form-control"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" style="width:150px; background-color: #454442; color: #f19539">Submit</button>
</form>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
